{
    "default_language": "default", 
    "id_string": "widgets", 
    "children": [
        {
            "name": "my_string", 
            "hint": "can be short or very long", 
            "label": "string widget", 
            "type": "string"
        }, 
        {
            "name": "my_int", 
            "hint": "try entering a number < 10", 
            "label": "integer widget", 
            "bind": {
                "jr:constraintMsg": "number must be less than 10", 
                "constraint": ". < 10"
            }, 
            "type": "int"
        }, 
        {
            "name": "my_decimal", 
            "hint": "only numbers > 10.51 and < 18.39", 
            "default": "18.31", 
            "label": "decimal widget", 
            "bind": {
                "jr:constraintMsg": "number must be between 10.51 and 18.39", 
                "constraint": ". > 10.51 and . < 18.39"
            }, 
            "type": "decimal"
        }, 
        {
            "name": "my_date", 
            "hint": "only future dates allowed", 
            "default": "2010-06-15", 
            "label": "date widget", 
            "bind": {
                "jr:constraintMsg": "only future dates allowed", 
                "constraint": ". >= today()"
            }, 
            "type": "date"
        }, 
        {
            "name": "my_time", 
            "hint": "testing time", 
            "label": "time widget", 
            "type": "time"
        }, 
        {
            "name": "my_select", 
            "hint": "don't pick c and d together", 
            "default": "a c", 
            "label": "select multiple widget", 
            "bind": {
                "jr:constraintMsg": "option c and d cannot be selected together", 
                "constraint": "not(selected(., 'c') and selected(., 'd'))"
            }, 
            "type": "select all that apply", 
            "children": [
                {
                    "name": "a", 
                    "label": "option a"
                }, 
                {
                    "name": "b", 
                    "label": "option b"
                }, 
                {
                    "name": "c", 
                    "label": "option c"
                }, 
                {
                    "name": "d", 
                    "label": "option d"
                }
            ]
        }, 
